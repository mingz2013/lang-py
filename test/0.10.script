def Man(){

    def this(){}

    def say(word){
        print(this.name, "say:", word)
    }

    this.name = "Man"
    this.age = 11
    this.say = say

    return this

}


def Child(){
    def this(){}
    this.super = Man()
    this.age = 1

    return this
}

a = Child()


a.super.say(111111)






